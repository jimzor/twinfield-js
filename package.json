{
  "name": "twinfield",
  "version": "0.0.21",
  "description": "A yet to become powerful and user-friendly Node.js SDK for interacting with the Twinfield accounting software's SOAP and OAuth APIs, enabling seamless integration and data exchange.",
  "main": "dist/cjs/index.js",
  "module": "/dist/esm/index.js",
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "node-fetch": "^2.7.0",
    "typescript": "^5.2.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.3",
    "@types/node-fetch": "^2.6.5",
    "@types/xml2js": "^0.4.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzor/twinfield-js.git"
  },
  "author": "Jimmy du Bois",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimzor/twinfield-js/issues"
  },
  "homepage": "https://github.com/jimzor/twinfield-js#readme"
}
